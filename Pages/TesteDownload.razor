@page "/teste-download"

<MudPaper Elevation="2">
    <MudButton Variant="Variant.Outlined" OnClick="DownloadScheduleAsync">TesteDownload</MudButton>
</MudPaper>

@code 
{
    private async Task DownloadScheduleAsync()
    {
        // Generate a file
        var fileUrl = "akaratefelgueiras/horarios.pdf";

        // Send the data to JS to actually download the file
        await JSRuntime.InvokeVoidAsync("downloadFile", fileUrl);
    }
}